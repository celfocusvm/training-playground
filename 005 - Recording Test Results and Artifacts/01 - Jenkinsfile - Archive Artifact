pipeline {
    agent { 
    	dockerfile {
            dir '005 - Recording Test Results and Artifacts'
            filename '01 - Dockerfile - Archive Artifact'
        }
    }
    stages {
        stage('build') {
            steps {
                sh "curl www.google.com"
                archiveArtifacts artifacts: 'google.html'
            }
        }
    }
}
